{
  "name": "generate",
  "version": "1.0.0",
  "description": "Generate workflow: interpret issue and create new issues",
  "workflow_type": "generate",
  "steps": [
    {
      "type": "agent",
      "name": "Interpret Issue",
      "agent": "issue-generator",
      "prompt": "$ARGUMENTS --adw-id ${adw_id}",
      "model": "base",
      "timeout": 600,
      "retry": {
        "max_retries": 2,
        "initial_delay": 5.0,
        "backoff": 2.0
      }
    }
  ]
}
