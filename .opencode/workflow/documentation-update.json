{
  "name": "documentation-update",
  "version": "1.0.0",
  "description": "Generate and update documentation using the documentation agent with dynamic prompts",
  "workflow_type": "custom",
  "steps": [
    {
      "type": "agent",
      "name": "Generate Documentation",
      "agent": "documentation",
      "prompt": "$ARGUMENTS --adw-id ${adw_id}",
      "model": "base",
      "timeout": 600,
      "retry": {
        "max_retries": 2,
        "initial_delay": 5.0,
        "backoff": 2.0
      }
    }
  ]
}