{
  "name": "execute-plan",
  "version": "1.0.0",
  "description": "Execute implementation plans using the execute-plan agent with isolated worktree execution",
  "workflow_type": "custom",
  "steps": [
    {
      "type": "agent",
      "name": "Execute Implementation Plan",
      "agent": "execute-plan",
      "prompt": "$ARGUMENTS --adw-id ${adw_id}",
      "model": "base",
      "timeout": 1200,
      "retry": {
        "max_retries": 3,
        "initial_delay": 10.0,
        "backoff": 2.0
      }
    }
  ]
}